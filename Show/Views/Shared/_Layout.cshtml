                    <img src="~/Images/beian_01.jpg">
                </a>
                <a href="javascript:;">
                    <img src="~/Images/beian_01.jpg">
                </a>
            </div>
        </div>
    </div>
    <!-- footer end-->
    <script src="~/Scripts/index.js" type="text/javascript"></script>
}
else
{
    @RenderBody()
}
</body>
</html>
