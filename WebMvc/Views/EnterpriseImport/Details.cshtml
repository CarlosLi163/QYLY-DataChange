@model RoadFlow.Data.Model.EnterpriseInfo
@{
    ViewBag.Title = "Details";
}
<form method="post">
    <table class="formtable" style="width:40%; margin-left:20px;">
        <tr>
            <th style="color:red;font-size:16px;font-weight:600; text-align:center;" colspan="2">企业信息</th>
        </tr>
        <tr>
            <th>企业名称</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>统一社会信用代码</th>
            <td>@Model.TYSHXYDM</td>
        </tr>
        <tr>
            <th>注册地</th>
            <td>@Model.ZCD</td>
        </tr>
        <tr>
            <th>注册资本</th>
            <td>@Model.ZCZB</td>
        </tr>
        <tr>
            <th>经营范围</th>
            <td>@Model.JYFW</td>
        </tr>
        <tr>
            <th>法人</th>
            <td>@Model.ArtificialPerson</td>
        </tr>
        <tr>
            <th>法人电话</th>
            <td>@Model.ArtificialPersonPhone</td>
        </tr>
        <tr>
            <th>成立日期</th>
            <td>@Model.CLRQ</td>
        </tr>
        <tr>
            <th>企业联系电话</th>
            <td>@Model.QYLXDH</td>
        </tr>
        <tr>
            <th>企业通讯地址</th>
            <td>@Model.QYTXDZ</td>
        </tr>
        <tr>
            <td></td>
            <td>
                <input type="button" id="close" name="close" value="返回" class="mybutton" style=" margin-left:10px;" />
            </td>
        </tr>
    </table>
</form>
<script type="text/javascript">
    var appid = '@Request.QueryString["appid"]';
    var iframeid = '@Request.QueryString["tabid"]';
    $(function () {
        $("#close").click(function () {
            location.href = '@Url.Action("Index")?appid=' + appid + "&tabid" + iframeid;
        });
    });
</script>
